<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
   <class name="com.fc.framework.web.entity.Role" table="t_roleinfo" lazy="false">
     <id name="roleid">
       <generator class="native"></generator>
     </id>
     <property name="rolename"></property>
     <property name="roledescribe"></property>
     <property name="createuser"></property>
     <property name="createdate"></property>
   </class>
</hibernate-mapping>
