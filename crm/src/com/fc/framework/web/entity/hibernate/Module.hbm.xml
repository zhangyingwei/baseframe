<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
   <class name="com.fc.framework.web.entity.Module" table="t_moduleinfo" lazy="false">
     <id name="moduleid">
       <generator class="native"></generator>
     </id>
     <property name="modulename"></property>
     <property name="fatherid"></property>
     <property name="modulepath"></property>
     <property name="createuser"></property>
     <property name="createdate"></property>
   </class>
</hibernate-mapping>
