	<bean id="loginAction" class="com.fc.framework.web.action.LoginAction" scope="prototype"></bean>
	
	<bean id="moduleAction" class="com.fc.framework.web.action.ModuleAction" scope="prototype"></bean>
	
	<bean id="userAction" class="com.fc.framework.web.action.UserAction" scope="prototype"></bean>
	
	<bean id="roleAction" class="com.fc.framework.web.action.RoleAction" scope="prototype"></bean>
	
